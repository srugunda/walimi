<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.navigationdrawermateriallibrary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_w_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_w_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WelcomeActivity"></activity>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CartActivity" />
        <activity
            android:name=".DetailsActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.navigationdrawermateriallibrary.MainActivity" />
        </activity>
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>